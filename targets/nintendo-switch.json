{
  "llvm-target": "aarch64",
  "build-tags": ["nintendoswitch", "arm64"],
  "goos": "linux",
  "goarch": "arm64",
  "compiler": "aarch64-none-elf-gcc",
  "linker": "aarch64-none-elf-gcc",
  "rtlib": "none",
  "libc": "none",
  "gc": "extalloc",
  "cflags": [
    "-march=armv8-a+crc+crypto",
    "-mtune=cortex-a57",
    "-mtp=soft",
    "-g",
    "-fPIE",
    "-Wall",
    "-ffunction-sections",
    "-D__SWITCH__",
    "-fno-rtti",
    "-fno-exceptions"
  ],
  "ldflags": [
    "-fPIE",
    "-g",
    "-march=armv8-a+crc+crypto",
    "-mtune=cortex-a57",
    "-mtp=soft",
    "-Wl,-z,text"
  ],
  "extra-files": [
    "src/runtime/runtime_nintendoswitch_helper.s"
  ]
}